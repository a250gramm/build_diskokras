{

  "page": "shino",  
  "servi": {
    "rowsSelector": ".fp04-field",
    "data": {
      "id": "bd",
      "sub_cat": "bd",
      "price": "input_number"
    }
  },
  "pay_met": {
    "radioName": "pay_method",
    "data": {
      "id": "bd",
      "sub_cat": "bd",
      "percent": "bd",
      "id_wallet": "bd"
    }
  },
  "calc":{
  "if":{
    "pay_met": {
       "id=pay_met_0": {
          "комментарий": "коммисии банка нет, план",
          "total_price":   ["sum", "servi.price", "number"],
          "total_percent": ["percent_of", "if.total_price", "pay_met.percent", "number", "negative"],
          "net_income":    ["sum", "if.total_price", "if.total_percent", "number"],
          "revenu_100":    ["sum", "if.total_price"],
          "revenu_fact":   ["percent_of", "if.total_price", "pay_met.percent", "number"],
          "revenu_plan":   ["sum", "if.total_price"],
          "fin_op": {
            "status":     ["text",   "plan"]
          },
          "sub_order": {
            "stat_pay":    ["text",   "none"],
            "stat_ready":  ["text",   "creat"]
          }
       },
       "id=pay_met_1": {
          "комментарий": "коммисии банка есть, факт",
          "total_price":   ["sum", "servi.price", "number"],
          "total_percent": ["percent_of", "if.total_price", "pay_met.percent", "number", "negative"],
          "net_income":    ["sum", "if.total_price", "if.total_percent", "number"],
          "revenu_100":    ["sum", "if.total_price"],
          "revenu_fact":   ["sum", "if.total_price"],
          "revenu_plan":   ["sum", "0", "number"],
          "fin_op": {
            "status":     ["text",   "fact"]
          },
          "sub_order": {
            "stat_pay":   ["text",   "full"],
            "stat_ready":  ["text",   "in_prog"]
          }
       }
    }
  }
  }
}
