        },
        "main_btn div_field-paymet input":{
            "position": ["absolute", "absolute", "absolute"],
            "inset": "0",
            "width": ["100%", "100%", "100%"],
            "height": ["100%", "100%", "100%"],
            "margin": "0",
            "opacity": [0, 0, 0],
            "cursor": ["pointer", "pointer", "pointer"],
            "z-index": [1, 1, 1]
