{
"html": {
    "width": [100,"%"],
    "box-sizing": "border-box",
    "font-family": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen, Ubuntu, Cantarell, sans-serif",
    "overflow-x": "hidden"
    },
"page": {
    "box-sizing": "border-box",
    "margin-top":      [0, 10, 5, "px"],
    "margin-left":     [0, 10, 5, "px"],
    "margin-bottom":   [0, 10, 5, "px"],
    "margin-right":    [0, 10, 5, "px"],
    "padding":  [0, 0, 0 , "px"],
    "overflow-y": "auto"
    },
"section": {
    "bg-color*": ["branded_1",50]
    },
"layout": {
    "device": {
        "desktop": {"width":"1200px", "flex-wrap": "nowrap", "gap": "0px"},
        "tablet":  {"width":"768px", "flex-wrap": "wrap",   "gap": "0px"},
        "mobile":  {"width":"576px", "flex-wrap": "wrap",   "gap": "0px"}
    },
    "margin":   ["0 auto", "px"],
    "padding":  ["10","px"],
    "border":   ["2px solid", "1", "yellow.150"],
    "bg-color*": ["red",50]
    },
"column": {
    "width":          "100%",
    "box-sizing":     "border-box",
    "display":        "flex",
    "flex-direction": "row",
    "gap":            ["2","px"],
    "margin":         ["0","px"],
    "padding":        ["5","px"],
    "border":         ["0px dotted", "1", "red.100"]
},
"col_in_row": {
    "display":        "flex",
    "flex-direction": "column",
    "flex-shrink":    "1",
    "flex-grow":      "0"
},
"row": {
    "width":          "100%",
    "box-sizing":     "border-box",
    "display":        "flex",
    "flex-direction": "row",
    "gap":            ["0","px"],
    "margin":         ["0","px"],
    "padding":        ["0","px"],
    "border":         ["0px dotted", "1", "blue.100"]
},
"group": {
    "box-sizing":     "border-box",
    "display":        "flex",
    "flex-direction": "row",
    "gap":            ["5","px"],
    "margin":         ["0","px"],
    "padding":        ["0","px"],
    "min-width":      "0",
    "max-width":      "100%",
    "width":          "100%"
},
"wrapper_content": {
    "box-sizing":     "border-box",
    "min-width":      "0",
    "max-width":      "100%",
    "width":          "auto",
    "flex":           "0 0 auto"
}


}